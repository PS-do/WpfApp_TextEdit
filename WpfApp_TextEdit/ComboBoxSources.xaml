<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:col="clr-namespace:System.Collections;assembly=mscorlib"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <col:ArrayList x:Key="fonts">
        <sys:String>Arial</sys:String>
        <sys:String>Verdana</sys:String>
        <sys:String>Times New Roman</sys:String>
    </col:ArrayList>
    <col:ArrayList x:Key="sizes">
        <sys:Int32>10</sys:Int32>
        <sys:Int16>12</sys:Int16>
        <sys:Double>14</sys:Double>
        <sys:Double>16</sys:Double>
        <sys:Double>20</sys:Double>
        <sys:Double>22</sys:Double>
        <sys:Double>23.45</sys:Double>
    </col:ArrayList>
    
</ResourceDictionary>